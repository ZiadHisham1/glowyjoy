<style>
.aboutHomeImage{
  max-width: 100px!important;
}
.aboutHomeImage-1{
  max-height: 250px!important;
  width:auto;
}
.home-about-area {
  padding-top: {{ section.settings.section_padding_top }}px;
  padding-bottom: {{ section.settings.section_padding_bottom }}px;
  {% if section.settings.section_bg_color_enable %}
  background-color: {{ section.settings.section_bg_color }};
  {% endif %}
  {% if section.settings.section_bg_image_enable %}
  background-repeat: no-repeat;
  background-position: center;
  background-size: cover;
  {% endif %}
  {% if section.settings.border_enable %}
  border-top: {{ section.settings.border }}px solid {{ section.settings.border_color | color_modify: 'alpha', section.settings.border_color_opacity }};
  {% endif %}
  margin-top: -80px;
  z-index: 1;
  position: relative;
}
@media (max-width: 1399px) {
  .home-about-area {
    margin-top: -50px;  
  }
}
@media (max-width: 1299px) {
  .home-about-area {
    margin-top: -30px;  
  }
}
@media (max-width: 1199px) {
  .home-about-area {
    padding-top: {{ section.settings.section_padding_top_ip }}px;
    padding-bottom: {{ section.settings.section_padding_bottom_ip }}px;
  }
}
@media (max-width: 991px) {
  .home-about-area {
    margin-top: 0px;  
  }
}
@media (max-width: 767px) {
  .home-about-area {
    padding-top: {{ section.settings.section_padding_top_xs }}px;
    padding-bottom: {{ section.settings.section_padding_bottom_xs }}px;
  }
}

/* main css */
.home-about-area .home-about-main {
  background-color: var(--extra-color-body);
  display: flex;
  flex-wrap: wrap;
  overflow: hidden;
  border-radius: var(--sti-border-radius);
}
.home-about-area .home-about-main .home-about-image {
  width: 33.33%;
}
.home-about-area .home-about-main .home-about-content {
  width: 66.67%;
}
@media (max-width: 1199px) {
  .home-about-area .home-about-main .home-about-image,
  .home-about-area .home-about-main .home-about-content {
    width: 100%;
  }
}

/* image css */
.home-about-area .home-about-main .home-about-image span.image-block svg {
  width: 100%;
}
{% if settings.enable_rtl %}
.home-about-area .home-about-main .home-about-image span.image-block img {
  transform: scaleX(-1);
}
{% endif %}

/* content css */
.home-about-area .home-about-main .home-about-content .content-main {
  padding: 0px 120px;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-evenly;
}
.home-about-area .home-about-main .home-about-content .content-main .about-content .section-capture {
  margin-bottom: 0px;
}
.home-about-area .home-about-main .home-about-content .content-main .about-content .section-capture .section-title {
  display: flex;
  align-items: center;
  {% if settings.enable_rtl %}
  text-align: right;
  {% else %}
  text-align: left;
  {% endif %}
}
.home-about-area .home-about-main .home-about-content .content-main .about-content .section-capture .section-title h2 {
  {% if settings.enable_rtl %}
  padding-left: 60px;
  {% else %}
  padding-right: 60px;
  {% endif %}
}
.home-about-area .home-about-main .home-about-content .content-main .about-content .section-capture .section-title p {
  {% if settings.enable_rtl %}
  margin-right: 0px;
  padding-right: 0px;
  {% else %}
  margin-left: 0px;
  padding-left: 0px;
  {% endif %}
}
.home-about-area .home-about-main .home-about-content .content-main .about-content .section-capture .section-title p::before {
  display: none;
}
.home-about-area .home-about-main .home-about-content .content-main .about-service ul.about-service-ul {
  display: flex;
  flex-wrap: wrap;
  {% if settings.enable_rtl %}
  margin-right: -30px;
  {% else %}
  margin-left: -30px;
  {% endif %}
  margin-top: -30px;
} 
.home-about-area .home-about-main .home-about-content .content-main .about-service ul.about-service-ul li.about-service-li {
  width: calc(33.33% - 30px);
  margin-top: 30px; background-color:transparent;
  {% if settings.enable_rtl %}
  margin-right: 30px;
  {% else %}
  margin-left: 30px;
  {% endif %}
}
.home-about-area .home-about-main .home-about-content .content-main .about-service ul.about-service-ul li.about-service-li .about-service-main {
  position: relative;
  background-color: transparent;
  padding: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  border-radius: var(--sti-border-radius);
  overflow: hidden;
}
.home-about-area .home-about-main .home-about-content .content-main .about-service ul.about-service-ul li.about-service-li .about-service-main::before {
  background-color: var(--font-color-primary);
  content: '';
  position: absolute;
  bottom: 0px;
  left: 0px;
  right: 0px;
  width: 100%;
  height: 5px;
  opacity: 0;
  visibility: hidden;
  -webkit-transition: opacity .3s ease-in-out, visibility .3s ease-in-out;
  -o-transition: opacity .3s ease-in-out, visibility .3s ease-in-out;
  transition: opacity .3s ease-in-out, visibility .3s ease-in-out;
}
.home-about-area .home-about-main .home-about-content .content-main .about-service ul.about-service-ul li.about-service-li:hover .about-service-main::before {
  opacity: 1;
  visibility: visible;
}
.home-about-area .home-about-main .home-about-content .content-main .about-service ul.about-service-ul li.about-service-li .about-service-main a.icon-block {
  color: var(--font-color-primary);
  font-size: 35px;
}
.home-about-area .home-about-main .home-about-content .content-main .about-service ul.about-service-ul li.about-service-li .about-service-main a.icon-block span.icon {
  line-height: 1;
  display: block;
}
.home-about-area .home-about-main .home-about-content .content-main .about-service ul.about-service-ul li.about-service-li .about-service-main a.title {
  font-size: 18px;
  margin-top: 23px;
  font-weight: var(--font-custom-weight-header);
} 
@media (max-width: 1399px) {
  .home-about-area .home-about-main .home-about-content .content-main {
    padding: 0px 50px;
  }
}
@media (max-width: 1199px) {
  .home-about-area .home-about-main .home-about-content .content-main {
    padding: 50px;
  }
  .home-about-area .home-about-main .home-about-content .content-main .about-service {
    margin-top: 35px;
  }
}
@media (max-width: 991px) {
  .home-about-area .home-about-main .home-about-content .content-main .about-content .section-capture .section-title {
    flex-direction: column;
    text-align: center;
  }
  .home-about-area .home-about-main .home-about-content .content-main .about-service ul.about-service-ul li.about-service-li .about-service-main a.title {
    font-size: 15px;
  }
}
@media (max-width: 767px) {
  .home-about-area .home-about-main .home-about-content .content-main {
    padding: 30px 12px;
  }
  .home-about-area .home-about-main .home-about-content .content-main .about-service {
    margin-top: 21px;
  }
  .home-about-area .home-about-main .home-about-content .content-main .about-service ul.about-service-ul {
    {% if settings.enable_rtl %}
    margin-right: -12px;
    {% else %}
    margin-left: -12px;
    {% endif %}
  }
  .home-about-area .home-about-main .home-about-content .content-main .about-service ul.about-service-ul li.about-service-li {
    width: calc(33.33% - 12px);
    {% if settings.enable_rtl %}
    margin-right: 12px;
    {% else %}
    margin-left: 12px;
    {% endif %}
  }
}
@media (max-width: 479px) {
  .home-about-area .home-about-main .home-about-content .content-main .about-content .section-capture .section-title h2 {
    {% if settings.enable_rtl %}
    padding-left: 0px;
    {% else %}
    padding-right: 0px;
    {% endif %}
  }
  .home-about-area .home-about-main .home-about-content .content-main .about-content .section-capture .section-title p {
    margin-top: 16px; 
  }
  .home-about-area .home-about-main .home-about-content .content-main .about-service ul.about-service-ul li.about-service-li {
    width: calc(100% - 12px);
  }
}
</style>