<section class="home-about-area" {% if section.settings.section_bg_image_enable %}style="background-image: url('{% if section.settings.section_bg_image %}{{ section.settings.section_bg_image | image_url }}{% else %}https://dummyimage.com/1920x800/f7f7f7/999999{% endif %}');"{% endif %}>
  <div class="container">
    <div class="row">
      <div class="col">
        <div class="home-about-main">
          <div class="home-about-image">
            <span class="image-block">
              {%- if section.settings.home_about_image -%}
              {{ section.settings.home_about_image | image_url: width: 1024 | image_tag:
              loading: 'lazy',
              width: section.settings.home_about_image.width,
              height: section.settings.home_about_image.height,
              class: "img-fluid image-1 aboutHomeImage-1",
              alt: section.settings.home_about_image.alt | escape }}
              {%- else -%}  
              {{ 'lifestyle-2' | placeholder_svg_tag: 'placeholder-svg' }}
              {%- endif -%}
            </span>
          </div>
          <div class="home-about-content">
            <div class="content-main">
              <div class="about-content">
                <div class="section-capture">
                  <div class="section-title">
                    {% if section.settings.home_about_title != '' %}
                    <h2 style="margin:10px; color:#000;">{{ section.settings.home_about_title }}</h2>
                    {% endif %}
                    {% if section.settings.home_about_desc != '' %}
                    <p class="desc">{{ section.settings.home_about_desc }}</p>
                    {% endif %}
                  </div>
                </div>
              </div>
              <div class="about-service">
                <ul class="about-service-ul">
                  {% for block in section.blocks %}
                  <li class="about-service-li">
                    <div class="about-service-main">    
                      <a href="{{ block.settings.service_url }}" >
                         <span class="image-block">
                            {%- if block.settings.service_icon_image -%}
                            {{ block.settings.service_icon_image | image_url: width: 1024 | image_tag:
                            loading: 'lazy',
                            width: block.settings.service_icon_image.width,
                            height: block.settings.service_icon_image.height,
                            class: "img-fluid image-1 aboutHomeImage",
                            style: "max-width:250px; max-height: 250px;",
                            alt: block.settings.service_icon_image.alt | escape }}
                            {%- else -%}  
                            {{ 'lifestyle-2' | placeholder_svg_tag: 'placeholder-svg' }}
                            {%- endif -%}
                         </span>
                      </a>
                      {% if block.settings.service_title != '' %}
                      <a href="{{ block.settings.service_url }}" style="font-weight:700; font-size: 25px;" class="title">{{ block.settings.service_title }}</a>
                      {% endif %}
                    </div>
                  </li>
                  {% endfor %}
                </ul>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</section> 

